{
  "name": "svelte-app",
  "version": "1.0.0",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "@babel/preset-env": "7.4.5",
    "mini-css-extract-plugin": "^1.3.4",
    "svelte-loader": "^3.0.0",
    "svelte": "^3.31.2",
    "webpack": "^5.16.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "dev": "webpack serve --content-base public",
    "test": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.mjs$": "babel-jest",
      "^.+\\.svelte$": "svelte-jester"
    },
    "transformIgnorePatterns": [
      "src/node_modules/(?!@sapper)"
    ],
    "moduleFileExtensions": [
      "js",
      "mjs",
      "svelte"
    ],
    "moduleDirectories": [
      "node_modules",
      "src/node_modules"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.3.8",
    "apollo-boost": "^0.4.9",
    "apollo-link-ws": "^1.0.20",
    "babel-jest": "^26.6.3",
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "fhirclient": "^2.3.10",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.11.0",
    "jest": "^26.6.3",
    "marked": "^2.0.0",
    "subscriptions-transport-ws": "^0.9.18",
    "svelte-apollo": "^0.4.0",
    "svelte-asyncable": "^1.3.1",
    "svelte-jester": "^1.3.0",
    "svelte-routing": "^1.5.0",
    "typewriter-editor": "^0.6.0"
  }
}
